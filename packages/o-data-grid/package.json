{
  "name": "o-data-grid",
  "version": "1.3.1",
  "description": "A React Data Grid and Query Builder for OData APIs. Based on the Material-UI DataGrid.",
  "main": "build/o-data-grid-cjs.js",
  "module": "build/o-data-grid-esm.js",
  "types": "build/o-data-grid.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "cd ../ && node_modules/.bin/rollup -c ./rollup.o-data-grid.config.mjs",
    "start": "cd ../ && node_modules/.bin/webpack-dev-server --mode development --open --hot -c ./webpack.o-data-grid.config.js",
    "build-dev": "cd ../ && node_modules/.bin/webpack -c ./webpack.o-data-grid.config.js",
    "lint": "cd ../ && node_modules/.bin/eslint ./o-data-grid"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamerst/o-data-grid.git"
  },
  "keywords": [
    "react",
    "react-component",
    "odata",
    "material-ui",
    "mui",
    "datagrid",
    "data-grid",
    "table",
    "datatable",
    "data-table",
    "filter",
    "query"
  ],
  "author": "James Tattersall",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/jamerst/o-data-grid/issues"
  },
  "homepage": "https://github.com/jamerst/o-data-grid#readme",
  "peerDependencies": {
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.0",
    "@mui/x-data-grid": "^6.0.0",
    "@mui/x-date-pickers": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "immutable": "^4.0.0",
    "recoil": "0.7.2",
    "tss-react": "3.6.2",
    "uuid": "^8.3.2"
  }
}
