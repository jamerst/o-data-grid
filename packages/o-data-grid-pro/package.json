  "description": "A React Data Grid and Query Builder for OData APIs. Based on the Material-UI DataGridPro.",
  "main": "build/o-data-grid-pro-cjs.js",
  "module": "build/o-data-grid-pro-esm.js",
  "types": "build/o-data-grid-pro.d.ts",
  "scripts": {
    "build": "cd ../ && node_modules/.bin/rollup -c ./rollup.o-data-grid-pro.config.js",
    "start": "cd ../ && node_modules/.bin/webpack-dev-server --mode development --open --hot -c ./webpack.o-data-grid-pro.config.js",
    "build-dev": "cd ../ && node_modules/.bin/webpack -c ./webpack.o-data-grid-pro.config.js",
    "lint": "cd ../ && node_modules/.bin/eslint ./o-data-grid"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamerst/o-data-grid.git"
  },
  "keywords": [
    "react",
    "react-component",
    "odata",
    "material-ui",
    "mui",
    "datagrid",
    "data-grid",
    "table",
    "datatable",
    "data-table",
    "filter",
    "query"
  ],
  "author": "James Tattersall",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/jamerst/o-data-grid/issues"
  },
  "homepage": "https://github.com/jamerst/o-data-grid#readme",
  "peerDependencies": {
    "@mui/icons-material": "^5.2.5",
    "@mui/lab": "^5.0.0-alpha.62",
    "@mui/material": "^5.2.8",
    "@mui/system": "^5.2.8",
    "@mui/x-data-grid-pro": "^5.4.0",
    "react": "^17.0.2"
  },
  "dependencies": {
    "immutable": "^4.0.0",
    "odata": "^1.3.1",
